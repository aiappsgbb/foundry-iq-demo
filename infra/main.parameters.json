{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "baseName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "environment": {
      "value": "${AZURE_ENVIRONMENT=dev}"
    },
    "repositoryUrl": {
      "value": "${AZURE_REPOSITORY_URL=https://github.com/aiappsgbb/foundry-iq-demo}"
    },
    "branch": {
      "value": "${AZURE_REPOSITORY_BRANCH=main}"
    },
    "repositoryToken": {
      "value": "${AZURE_REPOSITORY_TOKEN=}"
    },
    "deploySampleData": {
      "value": "${AZURE_DEPLOY_SAMPLE_DATA=true}"
    },
    "chatModelName": {
      "value": "${AZURE_CHAT_MODEL=gpt-4o-mini}"
    },
    "embeddingModelName": {
      "value": "${AZURE_EMBEDDING_MODEL=text-embedding-3-small}"
    }
  }
}
